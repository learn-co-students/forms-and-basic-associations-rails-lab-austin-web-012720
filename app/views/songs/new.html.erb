<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.collection_select :genre_name, Genre.all, :name, :name %><br>
  <p>Notes</p>
  <input name="song[note_contents][]"><br>
  <input name="song[note_contents][]"><br>
  <input name="song[note_contents][]"><br>

  <%= f.submit %>
<% end %>


